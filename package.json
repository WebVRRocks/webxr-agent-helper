{
  "name": "webxr-agent",
  "description": "A drop-in JavaScript library for handling presentation and navigation of WebXR experiences.",
  "version": "0.0.0",
  "source": "src/index.js",
  "module": "dist/webxr-agent.es.js",
  "main": "dist/webxr-agent.js",
  "umd:main": "dist/webxr-agent.umd.js",
  "files": [
    "src",
    "dist"
  ],
  "repository": "webvrrocks/webxr-agent",
  "bugs": {
    "url": "https://github.com/webvrrocks/webxr-agent/issues"
  },
  "homepage": "https://github.com/webvrrocks/webxr-agent",
  "license": "CC0-1.0",
  "authors": "WebXR Agent authors",
  "devDependencies": {
    "chai": "^3.5.0",
    "jsdom": "^9.12.0",
    "live-server": "^1.2.0",
    "microbundle": "^0.2.4",
    "mocha": "^3.2.0",
    "npm-run-all": "^4.1.2",
    "semver": "^5.3.0",
    "shelljs": "^0.7.8"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run watch",
